<!DOCTYPE html>
<html lang="tr" dir="ltr">
<link rel="stylesheet" href="bootstrap/css/bootstrap.css">
<!--font Awesome -->
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css" integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU" crossorigin="anonymous">


<!-- Ozel css dosyamız -->
<link rel="stylesheet" href="css/main.css">

<head>
  <meta charset="utf-8">
  <title>Halisaha Rezervasyon Sistemi </title>
</head>


<body>
  <!-- Navbar Bölümü -->
  <%-include("partials/header") %>
  <!-- Slider bölümü -->
  <div id="Carousel" class="carousel slide" data-ride="carousel">

    <ol class="carousel-indicators">
      <li data-target="#Carousel" data-slide-to="0" class="active"></li>
      <li data-target="#Carousel" data-slide-to="1"></li>
    </ol>

    <div class="carousel-inner">
      <div class="carousel-item active">
        <img class="d-block w-100" src="/images/halisaha-slider.png" alt="First slide">
      </div>
      <div class="carousel-item">
        <img class="d-block w-100" src="/images/halisaha-slider2.jpg" alt="Second slide">
      </div>
    </div>
    <a class="carousel-control-prev" href="#Carousel" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#Carousel" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>

  <!-- Sahalarımız  -->
  <div class="container">
    <div class="row halisaha-cards">
      <div class="col-lg-4 col-md-6">
        <div class="card" style="width: 18rem;">
          <img src="images/halisaha-slider2.jpg" class="card-img-top" alt="...">
          <div class="card-body">
            <h5 class="card-title">Açık Sahamız</h5>

            <a href="/uye-giris" class="btn btn-primary">Randevu Oluştur</a>
          </div>
        </div>
      </div>
      <div class="col-lg-4 col-md-6">
        <div class="card" style="width: 18rem;">
          <img src="images/halisaha-slider2.jpg" class="card-img-top" alt="...">
          <div class="card-body">
            <h5 class="card-title">Kapalı Sahamız</h5>

            <a href="/uye-giris" class="btn btn-primary">Randevu Oluştur</a>
          </div>
        </div>
      </div>
      <div class="col-lg-4 col-md-6">
        <div class="card" style="width: 18rem;">
          <img src="images/halisaha-slider2.jpg" class="card-img-top" alt="...">
          <div class="card-body">
            <h5 class="card-title">Isıtmalı Sahamız</h5>
            <a href="/uye-giris" class="btn btn-primary">Randevu Oluştur</a>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Duyurular -->
  <div class="jumbotron jumbotron-fluid">
    <div class="container">

      <h1 class="display-5"><i class="fas fa-bullhorn"></i>DUYURULAR</h1>

      <p class="lead">Ramazan Bayramının 3. gününde açığız,Bayram şekeriniz bizden!!</p>
      <p class="lead">Kurban bayramında 20.00 - 22.00 arasında <mark>50 LIRA</mark> indirim!</p>
    </div>
  </div>
  <br><br>


  <!--Reklam Banner-->

  <div class="container">
    <div class="row">
      <div class="col-lg-4">
        <div class="card">
          <img style="width: 100%; height: 150px;"
            src="data:image/png;base64,"
            class="card-img-top" alt="...">

        </div>
      </div>
      <div class="col-lg-4">
        <div class="card">
          <img style="width: 100%; height: 150px;"
            src="data:image/png;base64,"
            class="card-img-top" alt="...">

        </div>
      </div>
      <div class="col-lg-4">
        <div class="card">
          <img style="width: 100%; height: 150px;"
            src="data:image/png;base64,"
            class="card-img-top" alt="...">

        </div>
      </div>
    </div>
  </div>


  <!-- FOOTER BÖLÜMÜ -->
  <%-include("partials/footer") %>

  <script src="bootstrap/js/jquery.min.js" charset="utf-8"></script>
  <script src="bootstrap/js/bootstrap.min.js" charset="utf-8"></script>
  <script src="js/main.js" charset="utf-8"></script>
</body>

</html>